# Sorriso Inteligente App - Example Environment Variables

# API Configuration
VITE_API_BASE_URL=https://your-api-url.example.com
VITE_N8N_WEBHOOK_URL=https://your-n8n-webhook-url.example.com
VITE_EVOLUTION_API_URL=https://your-evolution-api-url.example.com

# Supabase Configuration
VITE_SUPABASE_URL=https://your-supabase-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_anon_key_here
VITE_SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Environment
VITE_ENVIRONMENT=development

# Debug Settings
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=debug

# Feature Flags
VITE_ENABLE_MOCK_API=false
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false

# Analytics & Monitoring
VITE_GA_TRACKING_ID=your_ga_tracking_id
VITE_SENTRY_DSN=your_sentry_dsn

# WhatsApp Business Configuration
VITE_CONFIG_SESSION_PHONE_CLIENT=your_phone_client_id
VITE_CONFIG_SESSION_PHONE_NAME=your_phone_name
VITE_CONFIG_SESSION_PHONE_VERSION=your_phone_version
VITE_WA_BUSINESS_JID=your_whatsapp_jid
